[user]
	name = "Marco Reinwarth"
	email = "marcoreinwarth@gmail.com"
[alias]
	co = checkout
[apply]
	whitespace = nowarn
[color]
	branch = auto
	diff = auto
	status = auto
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold
[color "status"]
	added = yellow
	changed = green
	untracked = cyan
[core]
	excludesfile = /Users/marco/.gitignore
[push]
	default = simple
